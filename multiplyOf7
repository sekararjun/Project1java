public class MultiplicationTable7 {

    public static void main(String[] args) {
        int number = 7; 
        int i = 1; 

        System.out.println("Multiplication Table of " + number + ":");
        while (i <= 10) {
            System.out.println(number + " * " + i + " = " + (number * i));
            i++;
        }
    }
}
