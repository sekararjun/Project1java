public class OddNumbersPrinter {
    public static void main(String[] args) {
        int limit = 20;
        System.out.println("Odd numbers from 1 to " + limit + ":");
        for (int i = 1; i <= limit; i++) {
            if (i % 2 != 0) {
                System.out.print(i + " ");
            }
        }
    }
}
